{
  "name": "@unidy.io/web-components",
  "description": "Unidy Web Components",
  "keywords": [
    "web-components",
    "unidy",
    "unidy-web-components",
    "unidy-sdk"
  ],
  "version": "0.0.1-alpha.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/unidy-web-components/unidy-web-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/unidy-web-components/unidy-web-components.esm.js",
      "require": "./dist/unidy-web-components/unidy-web-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UnidyID/unidy-sdk.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss": "^8.5.3",
    "puppeteer": "^24.3.0",
    "tailwindcss": "^4.1.5"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/postcss": "^2.1.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@unidy.io/sdk-api-client": "workspace:*",
    "chromium-bidi": "^4.1.1"
  }
}