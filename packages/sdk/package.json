{
  "name": "@unidy.io/sdk",
  "version": "1.0.0-alpha.1",
  "description": "Unidy SDK: API Client, Stencil Components, and React Wrappers",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist/", "loader/"],
  "scripts": {
    "clean": "rm -rf dist www loader .stencil",
    "build:api": "bun build src/api/*.ts --outdir dist/api --minify --sourcemap",
    "build:react": "biome lint --write . && bun build src/newsletter-react/index.tsx --outdir dist/newsletter-react --minify --sourcemap",
    "build:types": "tsc -p .",
    "build:stencil": "stencil build",
    "build": "bun run clean && bun run build:api && bun run build:stencil && bun run build:react && bun run build:types",
    "dev:api": "bun build src/api/*.ts --outdir dist/api --watch --sourcemap",
    "dev:stencil": "stencil build --dev --watch --serve",
    "dev:react": "bun build src/newsletter-react/index.tsx --outdir dist/newsletter-react --watch --sourcemap",
    "dev": "bunx concurrently \"bun run dev:api\" \"bun run dev:stencil\" \"bun run dev:react\"",
    "test": "stencil test --spec --e2e && jest",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/store": "^2.2.0",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@stencil/core": "^4.38.0",
    "@stencil/react-output-target": "^1.2.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/bun": "latest",
    "@types/events": "^3.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.8.0",
    "@types/react": "^19.1.10",
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.23.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "license": "MIT",
  "keywords": [
    "web-components",
    "unidy",
    "unidy-sdk",
    "unidy-web-components",
    "newsletter",
    "unidy-newsletter",
    "newsletter-sdk",
    "react",
    "next"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/sdk.esm.js",
      "require": "./dist/auth/sdk.cjs.js"
    },
    "./newsletter": {
      "import": "./dist/newsletter/sdk.esm.js",
      "require": "./dist/newsletter/sdk.cjs.js"
    },
    "./ticketable": {
      "import": "./dist/ticketable/sdk.esm.js",
      "require": "./dist/ticketable/sdk.cjs.js"
    },
    "./sdk-api-client": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs.js"
    },
    "./react": {
      "import": "./dist/newsletter-react/index.js",
      "require": "./dist/newsletter-react/index.cjs.js"
    }
  }
}
