{
  "name": "@unidy.io/sdk-api-client",
  "version": "0.1.5",
  "description": "Unidy API Client SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist",
    "build:js": "bun build src/*.ts --outdir dist --minify --sourcemap",
    "build:types": "tsc",
    "build": "bun run clean && bun run build:js && bun run build:types",
    "dev:js": "bun run watch src/*.ts --outdir dist --minify --sourcemap",
    "dev:types": "tsc --watch",
    "dev": "bunx concurrently \"bun run dev:js\" \"bun run dev:types\""
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.0.0"
  }
}
