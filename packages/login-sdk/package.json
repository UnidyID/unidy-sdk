{
  "name": "@unidy.io/auth",
  "version": "0.0.11-alpha",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "stencil build",
    "clean": "rm -rf dist www loader .stencil",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "jwt-decode": "4.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/unidy-login/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "unpkg": "./dist/unidy-login/index.esm.js"
    },
    "./lazy": {
      "import": "./dist/esm/unidy-login.js",
      "require": "./dist/cjs/unidy-login.js",
      "unpkg": "./dist/unidy-login/unidy-login.esm.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UnidyID/unidy-sdk.git"
  },
  "files": ["dist/"],
  "description": "Unidy Login SDK",
  "keywords": ["unidy-login", "unidy-auth", "unidy", "unidy-sdk"]
}
