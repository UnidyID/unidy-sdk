{
  "name": "@unidy.io/login-sdk",
  "version": "1.1.0",
  "description": "A lightweight SDK for handling authentication via iframe in web applications",
  "main": "dist/unidy-login.js",
  "types": "dist/index.d.ts",
  "module": "dist/unidy-login.min.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "bundle": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "types": "tsc",
    "build": "npm run clean && npm run bundle && npm run types",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "authentication",
    "oauth",
    "login",
    "iframe"
  ],
  "author": "Unidy Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.27.1",
    "babel-loader": "^9.1.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not dead",
              "not ie <= 11"
            ]
          }
        }
      ]
    ]
  }
}
