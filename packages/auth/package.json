{
  "name": "@unidy.io/auth",
  "version": "1.0.0-alpha.5",
  "publishConfig": {
    "tag": "next"
  },
  "description": "Unidy Component SDK for Authentication and User Management",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/auth/auth.esm.js",
  "exports": {
    ".": {
      "import": "./dist/auth/auth.esm.js",
      "require": "./dist/auth/auth.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": ["dist/", "loader/"],
  "scripts": {
    "build": "stencil build",
    "clean": "rm -rf dist www loader .stencil",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@unidy.io/sdk-api-client": "workspace:*",
    "@stencil/store": "^2.2.0",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.38.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.8",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.23.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "tailwindcss": "^4.1.7",
    "@tailwindcss/postcss": "^4.1.7"
  },
  "license": "MIT"
}
