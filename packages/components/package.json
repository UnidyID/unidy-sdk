{
  "name": "@unidy.io/components",
  "version": "0.2.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "stencil build",
    "clean": "rm -rf dist www loader .stencil",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@unidy.io/sdk-api-client": "workspace:*"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "jest-cli": "^29.7.0",
    "jest": "^29.7.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "tailwindcss": "^4.1.7"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/unidy-components/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "unpkg": "./dist/unidy-components/index.esm.js"
    },
    "./lazy": {
      "import": "./dist/esm/unidy-components.js",
      "require": "./dist/cjs/unidy-components.js",
      "unpkg": "./dist/unidy-components/unidy-components.esm.js"
    },
    "./newsletter": {
      "import": "./dist/esm/unidy-newsletter.js",
      "require": "./dist/cjs/unidy-newsletter.js",
      "unpkg": "./dist/per-component/unidy-newsletter.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UnidyID/unidy-sdk.git"
  },
  "files": ["dist/", "loader/"],
  "description": "Unidy SDK Components",
  "keywords": ["web-components", "unidy", "unidy-sdk", "unidy-web-components"]
}
